    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.1.0",
    "eslint": "^2.4.0",
    "eslint-config-cusxio": "0.0.3",
    "eslint-config-cusxio-react": "0.0.2",
    "mocha": "^2.4.5",
